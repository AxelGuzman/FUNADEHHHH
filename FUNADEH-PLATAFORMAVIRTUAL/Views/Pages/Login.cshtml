@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>INSPINIA | Login</title>

    <link href="~/Content/bootstrap.min.css" rel="stylesheet">
    <link href="~/Fonts/font-awesome/css/font-awesome.css" rel="stylesheet">

    <link href="~/Content/animate.css" rel="stylesheet">
    <link href="~/Content/style.css" rel="stylesheet">

</head>

<body class="gray-bg" background="~/images/lana.jpg">
  
        <div class="middle-box text-center loginscreen  animated fadeInDown">
            <div>

                <h2>Bienvenido </h2>
                <h2>Genesis-Funadeh</h2>


                <form class="m-t" role="form" action="#">
                    <div class="form-group">
                        <input type="email" class="form-control" placeholder="Correo Electrónico" required="">
                    </div>

                    <div class="form-group">
                        <input type="password" class="form-control" placeholder="Contraseña" required="">
                    </div>
                    <a class="btn btn-sm btn-white btn-block" href="@Url.Action("MenuPrincipal", "MenuPrincipal")">Ingresar</a>

                    <a href="#"><small>¿Has olvidado la contraseña?</small></a>
                    <p class="text-muted text-center"><small>¿No tienes una cuenta?</small></p>
                    <a class="btn btn-sm btn-primary btn-block" href="@Url.Action("Registros", "Registros")">Registrar</a>
                </form>
                <p class="m-t"> <small>Inspinia we app framework base on Bootstrap 3 &copy; 2015</small> </p>
            </div>
        </div>
   
</body>

</html>


